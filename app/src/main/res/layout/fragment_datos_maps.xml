<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".DatosMapsFragment" >

    <Spinner
        android:id="@+id/spComunidad"
        android:layout_width="294dp"
        android:layout_height="28dp"
        android:layout_marginBottom="32dp"
        android:entries="@array/Comunidad"
        app:layout_constraintBottom_toTopOf="@+id/spMunicipio"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Spinner
        android:id="@+id/spMunicipio"
        android:layout_width="294dp"
        android:layout_height="28dp"
        android:layout_marginBottom="40dp"
        android:entries="@array/Municipio"
        app:layout_constraintBottom_toTopOf="@+id/spZonaSan"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.991" />

    <Spinner
        android:id="@+id/spZonaSan"
        android:layout_width="294dp"
        android:layout_height="28dp"
        android:layout_marginBottom="50dp"
        android:entries="@array/ZonaSan"
        app:layout_constraintBottom_toTopOf="@+id/bBuscar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/bBuscar"
        android:layout_width="187dp"
        android:layout_height="42dp"
        android:text="Buscar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.44" />

    <TextView
        android:id="@+id/tvPoblacion"
        android:layout_width="104dp"
        android:layout_height="36dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="20dp"
        android:text="PoblaciÃ³n"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/tvContagios"
        app:layout_constraintEnd_toStartOf="@+id/etnPoblacion"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spMunicipio"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvContagios"
        android:layout_width="104dp"
        android:layout_height="36dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="20dp"
        android:text="Contagios"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/tvFallecidos"
        app:layout_constraintEnd_toStartOf="@+id/etnContagios"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spMunicipio"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/tvFallecidos"
        android:layout_width="104dp"
        android:layout_height="36dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:text="Fallecidos"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/etnFallecidos"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spMunicipio"
        app:layout_constraintVertical_bias="0.741" />

    <EditText
        android:id="@+id/etnPoblacion"
        android:layout_width="208dp"
        android:layout_height="44dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="15dp"
        android:ems="10"
        android:enabled="false"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/etnContagios"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.812"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spComunidad"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/etnContagios"
        android:layout_width="208dp"
        android:layout_height="44dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:layout_marginBottom="15dp"
        android:ems="10"
        android:enabled="false"
        android:inputType="number"
        app:layout_constraintBottom_toTopOf="@+id/etnFallecidos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.812"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spMunicipio"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/etnFallecidos"
        android:layout_width="208dp"
        android:layout_height="44dp"
        android:layout_marginStart="40dp"
        android:layout_marginLeft="40dp"
        android:ems="10"
        android:enabled="false"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.812"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spMunicipio"
        app:layout_constraintVertical_bias="0.737" />


</androidx.constraintlayout.widget.ConstraintLayout>

